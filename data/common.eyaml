---
# Decrypts to: 'testing eyaml encrypted secret'
test_password: ENC[PKCS7,]

azure_client_secret: ENC[PKCS7,]

# Convert the value for any key whom has the word 'password' in it to Sensitive data type
lookup_options:
  '^(.*)?password(.*)?$':
    convert_to: "Sensitive"
  '^(.*)?secret(.*)?$':
    convert_to: "Sensitive"
